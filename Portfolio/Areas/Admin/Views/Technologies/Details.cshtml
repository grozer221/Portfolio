@model TechnologyModel

@{
    ViewData["Title"] = $"Details {Model.Name} (AdminPanel)";
}

<h1>Details @Model.Name</h1>

<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class = "col-sm-10">
            @Model.Name
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Color)
        </dt>
        <dd class = "col-sm-10">
            @Model.Color
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateCreate)
        </dt>
        <dd class = "col-sm-10">
            @Model.DateCreate
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DateLastChange)
        </dt>
        <dd class = "col-sm-10">
            @Model.DateLastChange
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
